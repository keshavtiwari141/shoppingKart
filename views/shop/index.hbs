{{# each products}}
 <div class="row" >
 	{{# each this}}
	<div class="col-sm-6 col-md-4" >
		<img src={{this.imagePath}} alt="..." class="img-thumbnail">
		<h1>{{this.title}}</h1>
		<div class=align-left >${{this.price}} </div>
		<button type="button" class="btn btn-success">Buy</button>
	</div>
	{{/each}}
</div>
{{/each}}

